{
  "name": "data-grabber",
  "version": "0.1.0",
  "description": "A collection of scripts and utilities for pulling user data from various services",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "lint": "eslint .",
    "start": "node src/index.js",
    "integuru": "electron src/integuru/index.js",
    "dev": "NODE_ENV=development electron src/integuru/index.js",
    "build": "electron-builder",
    "build:mac": "electron-builder --mac",
    "build:win": "electron-builder --win",
    "build:linux": "electron-builder --linux"
  },
  "keywords": [
    "data",
    "export",
    "takeout",
    "backup"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "electron": "^36.0.1",
    "electron-builder": "^26.0.12",
    "eslint": "^8.47.0",
    "jest": "^29.6.2"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "commander": "^11.0.0",
    "dotenv": "^16.3.1",
    "playwright": "^1.52.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.5.3"
  },
  "build": {
    "appId": "com.datgrabber.app",
    "productName": "Data Grabber",
    "directories": {
      "output": "dist"
    },
    "files": [
      "src/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "mac": {
      "category": "public.app-category.utilities",
      "target": [
        "dmg",
        "zip"
      ]
    },
    "win": {
      "target": [
        "nsis",
        "portable"
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "category": "Utility"
    }
  }
}